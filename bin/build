#!/bin/bash

set -e

$(dirname $0)/go clean deaagent/deaagent
$(dirname $0)/go clean loggregator/loggregator
$(dirname $0)/go clean sourcehasher/sourcehasher
$(dirname $0)/go clean sinkhasher/sinkhasher

$(dirname $0)/go build -v deaagent/deaagent
$(dirname $0)/go build -v loggregator/loggregator
$(dirname $0)/go build -v sourcehasher/sourcehasher
$(dirname $0)/go build -v sinkhasher/sinkhasher
